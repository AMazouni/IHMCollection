package formes;

import java.awt.Point;

public class Rectangle extends Forme {

	Point a;
	Point c;
	Double longueur;
	Double largeur;
	public Rectangle() {
		super();
		// TODO Auto-generated constructor stub
	}
	public Rectangle(Point a, Point c, Double longueur, Double largeur) {
		super();
		this.a = a;
		
		this.c = c;
		this.longueur = longueur;
		this.largeur = largeur;
	}
	
}
